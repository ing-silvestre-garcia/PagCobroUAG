{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && npm run s3-sync && npm run cf-invalidate",
    "s3-sync":
      "aws s3 sync build/ s3://silverproductos.lareposteriagdl.com --profile serverless",
    "cf-invalidate":
      "aws cloudfront create-invalidation --distribution-id E1BH4BSXNOV1KV --paths /\\* --profile serverless"
  }
}
